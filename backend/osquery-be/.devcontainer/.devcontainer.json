{
  "name": "Run app in dev container",
  "build": {
    "context": "..",
    "dockerfile": "../Dockerfile",
    "target": "dev"
  },
  "forwardPorts": [8008],
  "customizations": {
    "vscode": {
      "extensions": [
        "charliermarsh.ruff",
        "editorconfig.editorconfig",
        "esbenp.prettier-vscode",
        "kevinrose.vsc-python-indent",
        "ms-python.python"
      ]
    }
  }
}
